{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cache": {
    "Endpoint": "localhost:6379",
    "EntitySettings": [
      {
        "Name": "BaseApi",
        "Expiry": "20",
        "Period": "s"
      }
    ]
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "MongoDB",
        "Args": {
          "databaseUrl": "mongodb://localhost:27017/test",
          "cappedMaxSizeMb": "1024",
          "cappedMaxDocuments": "50000"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=DESKTOP-EBTQAN3;Initial Catalog=dbtest;Integrated Security=True;TrustServerCertificate=True;"
  },
  "JwtConfig": {
    "Secret": "TestPrueba123456Asssa.TestPrueba123456Asssa.TestPrueba123456Asssa"
  },
  "OpenTelemetrySettings": {
    "SQLTracingSettings": {
      "SetDbStatementForText": true,
      "SetDbStatementForStoredProcedure": true
    },
    "ZipkinSettings": {
      "Endpoint": "http://localhost:9411/api/v2/spans"
    }
  }
}

